{
  "root": "apps/sporza-lite-static",
  "sourceRoot": "apps/sporza-lite-static",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "hugo",
        "cwd": "apps/sporza-lite-static"
      }
    },
    "serve-prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "hugo server",
        "cwd": "apps/sporza-lite-static"
      }
    },
    "serve-draft": {
      "executor": "nx:run-commands",
      "options": {
        "command": "hugo server -D",
        "cwd": "apps/sporza-lite-static"
      }
    }
  }
}
