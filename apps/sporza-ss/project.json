{
  "root": "apps/sporza-ss",
  "sourceRoot": "apps/sporza-ss/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o='../../dist/apps/sporza-ss/' ./src/main.go",
        "cwd": "apps/sporza-ss"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run ./src/main.go",
        "cwd": "apps/sporza-ss"
      }
    }
  }
}
